{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "test": "jest --watch --coverage",
    "cypress": "run-p start cypress:open",
    "cypress:open": "cypress open",
    "format": "prettier --write \"{src,lib}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:check": "prettier --list-different \"{src,lib}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:fix": "pretty-quick --staged"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.8",
    "@angular/cdk": "7.3.3",
    "@angular/common": "7.2.8",
    "@angular/compiler": "7.2.8",
    "@angular/core": "7.2.8",
    "@angular/forms": "7.2.8",
    "@angular/http": "7.2.8",
    "@angular/material": "7.3.3",
    "@angular/platform-browser": "7.2.8",
    "@angular/platform-browser-dynamic": "7.2.8",
    "@angular/router": "7.2.8",
    "@froko/ng-essentials": "1.13.1",
    "@ngxs/devtools-plugin": "3.4.2",
    "@ngxs/store": "3.4.2",
    "core-js": "2.6.5",
    "rxjs": "6.4.0",
    "tslib": "1.9.3",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-builders/jest": "7.3.1",
    "@angular-devkit/build-angular": "0.13.5",
    "@angular/cli": "7.3.5",
    "@angular/compiler-cli": "7.2.8",
    "@angular/language-service": "7.2.8",
    "@cypress/webpack-preprocessor": "4.0.3",
    "@types/jest": "24.0.9",
    "@types/node": "11.10.5",
    "babel-core": "6.26.3",
    "babel-jest": "24.3.1",
    "codelyzer": "4.5.0",
    "cypress": "3.1.5",
    "husky": "1.3.1",
    "jest": "24.3.1",
    "jest-preset-angular": "6.0.2",
    "ngx-wallaby-jest": "0.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "ts-loader": "5.3.3",
    "ts-node": "8.0.3",
    "tslint": "5.13.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix lint"
    }
  }
}
